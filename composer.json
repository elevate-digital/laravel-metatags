{
    "name": "elevate-digital/laravel-metatags",
    "description": "Add metatags to your eloquent models",
    "keywords": ["metatags", "seo", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Scheele",
            "email": "ian@elevatedigital.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "ElevateDigital\\Metatags\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElevateDigital\\Metatags\\MetatagsServiceProvider"
            ]
        }
    }
}
